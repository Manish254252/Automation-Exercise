{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "minItems": 1,
  "items": {
    "type": "object",
    "required": [
      "id",
      "name",
      "company",
      "username",
      "email",
      "address",
      "zip",
      "state",
      "country",
      "phone"
    ],
    "properties": {
      "id": {
        "type": "integer",
        "minimum": 1
      },
      "name": {
        "type": "string",
        "minLength": 1
      },
      "company": {
        "type": "string",
        "minLength": 1
      },
      "username": {
        "type": "string",
        "pattern": "^[a-zA-Z0-9_]+$"
      },
      "email": {
        "type": "string",
        "format": "email"
      },
      "address": {
        "type": "string"
      },
      "zip": {
        "type": "string",
        "minLength": 3
      },
      "state": {
        "type": "string"
      },
      "country": {
        "type": "string"
      },
      "phone": {
        "type": "string"
      },
      "photo": {
        "type": "string",
        "format": "uri"
      }
    },
    "additionalProperties": false
  }
}
